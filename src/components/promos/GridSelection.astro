---
import { lists } from "@/libs/data/lists";
import CardSelection from "../cards/CardSelection.astro";

const keys = Object.keys(lists);

// const defaultLogoPath = firms.FTMO.logoText;
---

<div class="grid grid-cols-4 w-1200px p-[80px] gap-[40px]">
  {
    keys.map((key) => (
      <CardSelection
        name={lists[key].name}
        shortDescription={lists[key].shortDescription}
      />
    ))
  }
</div>
